          "addressDetails": {
            "$id": "/properties/customerDetails/items/properties/addressDetails",
            "type": "object",
            "properties": {
              "addressLine1": {
                "$id": "/properties/customerDetails/items/properties/addressDetails/properties/addressLine1",
                "type": "string",
